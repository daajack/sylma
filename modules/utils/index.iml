<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/interface" xmlns:ls="http://www.sylma.org/security">
  <name>Utils</name>
  <file>Utils.php</file>
  <method path="analyze-scripts" return-default="true" name="analyzeScripts"/>
  <method path="analyze-documents" return-default="true" name="analyzeDocuments"/>
  <method path="clean-document" return-default="true" name="cleanDocument">
    <argument format="php-string" name="path"/>
  </method>
  <method name="testAction" path="test-action" ls:owner="root" ls:group="0" ls:mode="700">
    <argument format="php-string" name="path"/>
    <argument required="false" format="php-integer" name="time" default="10"/>
    <argument required="false" format="php-integer" name="count"/>
  </method>
</class>
