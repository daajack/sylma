<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml" xmlns:la="http://www.sylma.org/processors/action-builder" xmlns:ins="http://www.sylma.org/modules/inspector">
  <le:settings>
    <le:name>Inspector</le:name>
  </le:settings>
  <le:window>
    <li:add-css le:format="string">
      <le:directory/>
      <le:argument>/index.css</le:argument>
    </li:add-css>
  </le:window>
  <div class="clearfix">
    <le:action path="module">
      <le:argument name="path">/sylma/core/module/old/settings.yml</le:argument>
      <le:argument name="title">Module</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/core/settings/settings.yml</le:argument>
      <le:argument name="title">Settings</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/action/action.yml</le:argument>
      <le:argument name="title">Action</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/dom/settings.yml</le:argument>
      <le:argument name="title">DOM</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/schemas/settings.yml</le:argument>
      <le:argument name="title">Schemas</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/window/settings.yml</le:argument>
      <le:argument name="title">Window</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/storage/fs/settings.yml</le:argument>
      <le:argument name="title">File system</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/core/sylma.yml</le:argument>
      <le:argument name="title">Sylma</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/modules/inspector/settings.yml</le:argument>
      <le:argument name="title">Inspector</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/core/old/exist.yml</le:argument>
      <le:argument name="title">eXist</le:argument>
    </le:action>
    <le:action path="module">
      <le:argument name="path">/sylma/core/old/html.yml</le:argument>
      <le:argument name="title">HTML</le:argument>
    </le:action>
  </div>
  <le:action path="interface/get-declared">
    <li:parse>
      <le:template>
				<xsl:stylesheet version="1.0" extension-element-prefixes="func" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ins="http://www.sylma.org/modules/inspector" xmlns:func="http://exslt.org/functions" xmlns:set="http://exslt.org/sets" ins:ns="null">
				  <xsl:template match="/*">
				    <p>
				      <xsl:apply-templates/>
			      </p>
				  </xsl:template>
				  <xsl:template match="ins:class">
			      <a href="[$sylma-directory]/class/{.}"><xsl:value-of select="."/></a>
				  </xsl:template>
				</xsl:stylesheet>
      </le:template>
    </li:parse>
  </le:action>
</le:action>
