<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/parser/caller" xmlns:ls="http://www.sylma.org/security">
  <namespace>\sylma\parser\xslt</namespace>
  <name>Handler</name>
  <method path="set-parameter" name="setParameter" return="php-boolean">
    <argument name="name" format="php-string"/>
    <argument name="value" format="php-string"/>
  </method>
  <method path="parse" name="parseDocument" return="\sylma\dom\document">
    <argument name="document" format="\sylma\dom\document"/>
  </method>
</class>
