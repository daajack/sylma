<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/interface">
  <name>Action_Array</name>
  <method return-default="true" name="implode" path="implode">
    <argument name="separator" format="php-string"/>
  </method>
  <method return-default="true" name="item" path="item">
    <argument name="id" required="false">
      <format>php-string</format>
      <format>php-integer</format>
    </argument>
  </method>
</class>
