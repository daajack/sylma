<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  <le:settings>
    <le:name>Formulaire</le:name>
    <le:template path="form.xsl" set-variable="template">
      <li:set-parameter>
        <le:argument>action</le:argument>
        <le:get-argument name="action"/>
      </li:set-parameter>
      <li:set-parameter>
        <le:argument>form-id</le:argument>
        <le:get-argument name="form-id"/>
      </li:set-parameter>
    </le:template>
    <le:argument format="php-string" name="form-id"/>
    <le:argument format="XML_Document" name="model"/>
    <le:argument format="php-string" name="action"/>
    <le:argument name="template-extension" required="false" format="XML_Element">
      <le:validate>
        <le:get-variable name="template">
          <li:include-element>
            <le:get-argument name="template-extension"/>
          </li:include-element>
        </le:get-variable>
      </le:validate>
    </le:argument>
  </le:settings>
  <le:action path="/modules/calendar"/>
  <le:window>
    <li:add-css le:format="string">
      <le:directory/>
      <le:string>/form.css</le:string>
    </li:add-css>
    <li:add-js le:format="string">
      <le:directory/>
      <le:string>/form.js</le:string>
    </li:add-js>
  </le:window>
  <le:get-argument name="model">
    <li:parse>
      <le:get-variable name="template"/>
    </li:parse>
  </le:get-argument>
</le:action>
