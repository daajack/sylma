<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/interface" xmlns:ls="http://www.sylma.org/security">
  <name>XML_Path</name>
  <method-construct>
    <argument name="path" format="php-string"/>
    <argument required="false" name="arguments" format="php-array"/>
    <argument format="php-boolean" required="false" name="parse"/>
  </method-construct>
  <method return-default="true" name="getIndex" path="get-index">
    <argument name="key" format="php-integer" required="false"/>
    <argument name="remove" format="php-boolean" required="false"/>
  </method>
  <method return-default="true" name="getDirectory" path="get-directory"/>
  <method return-default="true" name="getAllIndex" path="get-all-index">
    <argument name="remove" format="php-boolean" required="false"/>
  </method>
  <method return-default="true" name="getAssoc" path="get-assoc">
    <argument name="key" format="php-string" required="false"/>
    <argument name="remove" format="php-boolean" required="false"/>
  </method>
  <method return-default="true" path="get-simple" name="getSimplePath"/>
  <method return-default="true" path="get-original" name="getOriginalPath"/>
  <method return-default="true" path="get-action" name="getActionPath"/>
  <method return-default="true" path="get-extension" name="getExtension"/>
</class>
