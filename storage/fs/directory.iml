<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/parser/caller" xmlns:ls="http://www.sylma.org/security">
  <namespace>\sylma\storage\fs</namespace>
  <name>directory</name>
  <method path="get-directory" name="getDirectory" return="directory">
    <argument name="path" format="php-string"/>
    <argument name="debug" format="php-boolean" required="false"/>
  </method>
  <method path="get-file" name="getFile" return="file">
    <argument name="path" format="php-string"/>
    <argument name="debug" format="php-boolean" required="false"/>
  </method>
</class>
