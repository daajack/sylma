<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml" xmlns:la="http://www.sylma.org/processors/action-builder">
  <le:settings>
    <le:name>Explorer v2.0</le:name>
    <le:processor path="/sylma/processors/action-builder" namespace="http://www.sylma.org/processors/action-builder"/>
  </le:settings>
  <le:action path="/sylma/processors/action-builder/init"/>
  <le:window>
    <li:add-js le:format="string">
      <le:directory/>
      <le:argument>/explorer.js</le:argument>
    </li:add-js>
    <li:add-css le:format="string">
      <le:directory/>
      <le:argument>/explorer.css</le:argument>
    </li:add-css>
  </le:window>
  <la:object name="explorer" extend-base="sylma.explorer-classes" class="explorer">
    <la:property name="pathInterface">
      <le:string>
        <le:directory/>
        <le:argument>/interface</le:argument>
      </le:string>
    </la:property>
    <le:action path="/modules/lightbox/cerabox"/>
    <div id="explorer">
      <le:action path="tools/resource"/>
      <le:action path="tree"/>
      <le:action path="mozaic"/>
      <le:action path="tools/add"/>
    </div>
  </la:object>
</le:action>
