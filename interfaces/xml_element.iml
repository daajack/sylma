<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/interface">
  <name>XML_Element</name>
  <method-construct>
    <argument format="php-string" name="name"/>
    <argument name="children" required="false">
      <format>php-array</format>
      <format>php-null</format>
      <format>php-string</format>
      <format>XML_Document</format>
      <format>XML_Element</format>
      <format>XML_NodeList</format>
    </argument>
    <argument format="php-array" name="attributes" required="false" allow-null="true"/>
    <argument format="php-string" name="namespace" required="false" allow-null="true"/>
  </method-construct>
  <method path="view" name="view" return-default="true"/>
  <method path="get-children" name="getChildren" return-default="true"/>
  <method return-default="true" path="has-children" name="hasChildren"/>
  <method path="get" name="get" output="XML_Element" return-default="true">
    <argument name="path" format="php-string"/>
    <argument name="namespaces" format="php-array" required="false"/>
  </method>
  <method path="get-first" name="getFirst" output="XML_Element"/>
  <method path="get-last" name="getLast" output="XML_Element"/>
  <method path="read" name="read" output="php-string" return-default="true">
    <argument name="path" format="php-string" required="false"/>
    <argument name="namespaces" format="php-array" required="false"/>
  </method>
  <method path="query" name="query" output="XML_NodeList">
    <argument name="path" format="php-string"/>
    <argument name="namespaces" format="php-array" required="false"/>
  </method>
  <method path="set" name="set">
    <multiple-arguments required-count="1"/>
  </method>
  <method path="add" name="add" output="XML_Element">
    <multiple-arguments/>
  </method>
  <method output="XML_Element" path="shift" name="shift">
    <multiple-arguments/>
  </method>
  <method path="insert" name="insert" output="XML_Element">
    <argument name="path" format="XML_Element"/>
    <argument name="next-sibling" format="XML_Element" required="false"/>
  </method>
  <method path="set-attribute" name="setAttribute" output="php-string">
    <argument name="name" format="php-string"/>
    <argument name="value" required="false">
      <format>php-float</format>
      <format>php-integer</format>
      <format>php-string</format>
      <format>php-null</format>
    </argument>
  </method>
  <method path="move" name="move">
    <argument format="XML_Element" name="target"/>
  </method>
  <method path="replace" name="replace">
    <argument format="XML_Element" name="target"/>
  </method>
  <method path="get-document" name="getDocument" return-default="true"/>
  <method path="parse" name="parseXSL" return-default="true">
    <argument name="template">
      <format>XML_Document</format>
      <format>XSL_Document</format>
    </argument>
  </method>
</class>
