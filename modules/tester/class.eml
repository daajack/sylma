<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml" xmlns:test="http://www.sylma.org/modules/tester">
  <le:settings>
    <le:name>Test class</le:name>
    <le:argument format="XML_Document"/>
  </le:settings>
  <le:window>
    <li:add-css le:format="string">
      <le:directory/>
      <le:argument>/index.css</le:argument>
    </li:add-css>
  </le:window>
  <div class="sylma-tests sylma-tests-class">
    <le:get-argument return="false">
      <li:parse>
        <le:template path="index.xsl"/>
      </li:parse>
    </le:get-argument>
  </div>
</le:action>
