<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/parser/action" xmlns="http://www.w3.org/1999/xhtml" xmlns:lm="http://www.sylma.org/parser/action/basic" xmlns:li="http://www.sylma.org/parser/caller" xmlns:tst="http://www.sylma.org/modules/tester">
  <le:settings>
    <le:argument format="php-string" name="file" required="false"/>
  </le:settings>
  <le:context name="title">Test actions grouped</le:context>
  <le:action path="/sylma/modules/tester/class">
    <le:document>
      <tst:tests>
        <le:interface path="interface">
          <le:get-argument name="file"/>
        </le:interface>
      </tst:tests>
    </le:document>
  </le:action>
</le:action>
