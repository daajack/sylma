<arg:argument
  xmlns:arg="http://2013.sylma.org/core/argument"
  xmlns="http://2013.sylma.org/template/binder"
  xmlns:cls="http://2013.sylma.org/core/factory"
>
  <arg:import>/#sylma/parser/languages/js/window.xml</arg:import>
  <debug>
    <show>0</show>
  </debug>
  <mootools>/#sylma/ui/mootools-min.js</mootools>
  <cls:classes>
    <cls:base>\sylma\template\binder</cls:base>
    <cls:class alias="class" name="_class\Template">
      <cls:classes>
        <cls:class alias="object" name="_Object"/>
      </cls:classes>
    </cls:class>
    <component>
      <cls:classes>
        <cls:class alias="event" name="component\Event"/>
        <cls:class alias="method" name="component\Method"/>
        <cls:class alias="option" name="component\Option"/>
        <cls:class alias="node" name="component\Node"/>
        <cls:class alias="include" name="component\_Include"/>
        <cls:class alias="name" name="component\Name"/>
      </cls:classes>
    </component>
    <context>
      <cls:classes>
        <cls:class alias="classes" name="context\Classes"/>
        <cls:class alias="objects" name="context\Objects"/>
      </cls:classes>
    </context>
  </cls:classes>
</arg:argument>
