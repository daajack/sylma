<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml" xmlns:ls="http://www.sylma.org/security">
  <le:settings>
    <le:name>Initialisation du processeur ActionBuilder</le:name>
  </le:settings>
  <le:window>
    <li:add-js>
      <le:get-settings>javascript/mootools</le:get-settings>
    </li:add-js>
    <li:add-js>
      <le:get-settings>javascript/mootools-more</le:get-settings>
    </li:add-js>
    <li:add-js le:format="string">
      <le:directory/>
      <le:argument>/sylma.js</le:argument>
    </li:add-js>
    <li:add-css le:format="string">
      <le:directory/>
      <le:argument>/sylma.css</le:argument>
    </li:add-css>
    <li:add-js ls:owner="root" ls:group="0" ls:mode="710">
      <le:null/>
      <le:argument>SYLMA_IS_ADMIN = true;</le:argument>
    </li:add-js>
  </le:window>
</le:action>
