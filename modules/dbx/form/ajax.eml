<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:la="http://www.sylma.org/processors/action-builder">
  <le:settings>
    <le:name>Formulaire</le:name>
    <le:processor path="/sylma/processors/action-builder" namespace="http://www.sylma.org/processors/action-builder"/>
    <le:argument format="XML_Document" name="model"/>
    <le:argument format="XSL_Document" name="template"/>
    <le:argument format="php-string" name="module"/>
  </le:settings>
  <le:action path="/sylma/processors/action-builder/init"/>
  <le:action path="/modules/calendar"/>
  <le:window>
    <li:add-css le:format="string">
      <le:directory/>
      <le:string>/form.css</le:string>
    </li:add-css>
    <li:add-js le:format="string">
      <le:directory/>
      <le:string>/form-ajax.js</le:string>
    </li:add-js>
  </le:window>
  <la:layer extend-base="sylma.dbx" class="form">
    <la:property name="pathAdd">
      <le:string>
        <le:get-argument name="module"/>
        <le:argument>/add-field</le:argument>
      </le:string>
    </la:property>
    <la:property name="pathUpload">
      <le:string>
        <le:get-argument name="module"/>
        <le:argument>/upload</le:argument>
      </le:string>
    </la:property>
    <la:property name="pathUploadView">
      <le:string>
        <le:get-argument name="module"/>
        <le:argument>/upload-view</le:argument>
      </le:string>
    </la:property>
    <le:get-argument name="model">
      <li:parse>
        <le:get-argument name="template"/>
      </li:parse>
    </le:get-argument>
  </la:layer>
</le:action>
