<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/interface">
  <name>XML_Document</name>
  <extends>XML_Element</extends>
  <method-construct>
    <argument name="path" required="false">
      <format>php-string</format>
      <format>XML_Element</format>
      <format>php-null</format>
      <format>XML_Document</format>
    </argument>
  </method-construct>
  <method path="get" name="get" return-default="true">
    <argument name="path" format="php-string"/>
    <argument name="namespaces" format="php-array" required="false"/>
  </method>
  <method path="read" name="read" return-default="true">
    <argument name="path" format="php-string"/>
    <argument name="namespaces" format="php-array" required="false"/>
  </method>
  <method path="query" name="query" return-default="true">
    <argument name="path" format="php-string"/>
    <argument name="namespaces" format="php-array" required="false"/>
  </method>
  <method path="get-root" name="getRoot" return-default="true"/>
  <method path="is-empty" name="isEmpty" return-default="true"/>
  <method path="set" name="set">
    <multiple-arguments required-count="1"/>
  </method>
  <method path="add" name="add">
    <multiple-arguments/>
  </method>
  <method path="insert" name="insert">
    <argument name="path" format="XML_Element"/>
    <argument name="next-sibling" format="XML_Element" required="false"/>
  </method>
  <method path="parse" name="parseXSL" return-default="true">
    <argument name="document" format="XML_Document"/>
    <argument name="xml" format="php-boolean" required="false"/>
  </method>
  <method path="get-model" name="getModel" return-default="true">
    <argument name="schema" format="XML_Document"/>
    <argument name="options" format="php-array" required="false"/>
  </method>
  <method path="include-externals" name="includeExternals"/>
  <method path="view" name="view" return-default="true">
    <argument name="inline" format="php-boolean" required="false"/>
  </method>
  <method name="dspm" path="dsp"/>
</class>
