<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/interface">
  <file>XML.php</file>
  <name>XSL_Document</name>
  <extends>XML_Document</extends>
  <method path="get-parameter" name="getParameter">
    <argument format="php-string" name="local-name"/>
  </method>
  <method path="remove-parameter" name="removeParameter">
    <argument format="php-string" name="local-name"/>
  </method>
  <method path="set-parameters" name="setParameters">
    <argument format="php-array" name="parameters"/>
  </method>
  <method path="set-parameter" name="setParameter">
    <argument format="php-string" name="name"/>
    <argument name="value">
      <format>php-string</format>
      <format>php-integer</format>
    </argument>
  </method>
  <method path="include-element" name="includeElement">
    <argument name="element" format="XML_Element"/>
  </method>
  <method path="parse" name="parseDocument" return-default="true">
    <argument name="document" format="XML_Document"/>
  </method>
  <method path="include-external" name="includeExternal">
    <argument name="template" format="XSL_Document" allow-null="true"/>
    <argument name="element" format="XML_Element" required="false"/>
    <argument name="marks" format="php-array" required="false"/>
  </method>
</class>
