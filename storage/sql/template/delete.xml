<arg:argument
  xmlns:arg="http://2013.sylma.org/core/argument"
  xmlns="http://2013.sylma.org/sql/template"
  xmlns:cls="http://2013.sylma.org/core/factory"
>
  <arg:import>builder.xml</arg:import>
  <cls:classes>
    <cls:base>\sylma\storage\sql</cls:base>
    <cls:class alias="elemented" name="template\handler\Basic">
      <cls:classes>
        <component>
          <cls:classes>
            <cls:base>\sylma\storage\sql\template</cls:base>
            <cls:class alias="table" name="delete\Table"/>
            <cls:class alias="field" name="view\Field"/>
            <cls:class alias="foreign" name="view\Foreign"/>
            <cls:class alias="reference" name="view\Reference"/>
          </cls:classes>
        </component>
      </cls:classes>
    </cls:class>
  </cls:classes>
</arg:argument>

