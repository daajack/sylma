<?xml version="1.0" encoding="utf-8"?>
<le:action
  xmlns:le="http://www.sylma.org/parser/action"
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:lm="http://www.sylma.org/parser/action/basic"
  xmlns:test="http://www.sylma.org/modules/tester"
>
  <le:settings>
    <le:document le:set-variable="doc">
      <test:tests>
        <le:action path="/#sylma/core/argument/test/group"/>
        <le:action path="/#sylma/core/factory/test/group"/>
        <le:action path="/#sylma/storage/fs/test/group"/>
        <le:action path="/#sylma/parser/action/test/group"/>
        <le:object class="\sylma\action\test\Handler"/>
        <le:action path="/#sylma/schema/test/group"/>
        <le:object class="\sylma\storage\sql\test\Handler"/>
        <le:action path="/#sylma/view/test/group"/>
        <le:object class="\sylma\storage\xml\test\Handler"/>
        <le:object class="\sylma\view\parser\crud\test\grouped\Grouped"/>
        <le:object class="\sylma\parser\security\test\Module"/>
        <le:object class="\sylma\core\module\test\Basic"/>
        <le:object class="\sylma\dom\test\Basic"/>
        <le:object class="\sylma\dom\argument\test\Basic"/>
        <le:object class="\sylma\core\test\Handler"/>
        <le:object class="\sylma\device\test\Handler"/>
      </test:tests>
    </le:document>
  </le:settings>
  <le:context name="title">Tests</le:context>
  <le:context name="css">
    <lm:file path="index.css"/>
  </le:context>
  <div class="sylma-tests">
    <le:action path="/#sylma/template/binder/test"/>
    <lm:template path="index.xsl">
      <le:call name="parse-document">
        <le:get-variable name="doc"/>
      </le:call>
    </lm:template>
  </div>
</le:action>
